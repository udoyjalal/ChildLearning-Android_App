<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundmain">

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageButton13"
        android:layout_marginStart="185dp"
        android:background="@null"
        android:contentDescription="@string/todo"
        android:onClick="goBird"
        android:src="@mipmap/eagle"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="185dp" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageButton12"
        android:layout_marginEnd="132dp"
        android:background="@null"
        android:contentDescription="@string/todo"
        android:onClick="goBird"
        android:src="@mipmap/peacock"
        android:layout_alignParentRight="true"
        android:layout_marginRight="132dp" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="49dp"
        android:layout_marginTop="343dp"
        android:background="@null"
        android:contentDescription="@string/todo"
        android:onClick="goBird"
        android:src="@mipmap/heron"
        android:layout_alignParentRight="true"
        android:layout_marginRight="49dp" />

    <ImageButton
        android:layout_width="200px"
        android:layout_height="200px"
        android:src="@mipmap/cat"
        android:background="@null"
        android:onClick="goAnimal"
        android:layout_alignTop="@+id/imageButton15"
        android:layout_alignLeft="@+id/imageButton11"
        android:layout_alignStart="@+id/imageButton11"
        android:contentDescription="@string/todo" />
    <ImageButton
        android:layout_width="200px"
        android:layout_height="200px"
        android:src="@mipmap/monkey"
        android:background="@null"
        android:onClick="goAnimal"
        android:layout_alignTop="@+id/imageButton14"
        android:layout_alignLeft="@+id/imageButton13"
        android:layout_alignStart="@+id/imageButton13"
        android:id="@+id/imageButton15"
        android:contentDescription="@string/todo" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_alignStart="@+id/imageButton12"
        android:layout_below="@+id/imageButton11"
        android:background="@null"
        android:contentDescription="@string/todo"
        android:onClick="goAnimal"
        android:src="@mipmap/elephant"
        android:layout_alignLeft="@+id/imageButton12" />

    <com.google.android.gms.ads.AdView xmlns:ads="http://schemas.android.com/apk/res-auto"
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="244dp"
        android:layout_marginEnd="65dp"
        ads:adSize="BANNER"
        ads:adUnitId="ca-app-pub-3940256099942544/6300978111"
        android:layout_alignParentRight="true"
        android:layout_marginRight="65dp"></com.google.android.gms.ads.AdView>

</RelativeLayout>